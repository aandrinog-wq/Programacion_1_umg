Algoritmo Edad
	// Variables
	// Cambio de variables
	Definir actual, ANOS, nacimiento Como Entero
	// Asignacion de datos
	actual <- 2026
	ANOS <- 0
	nacimiento <- 0
	// ingreso de datos
	Escribir 'Ingrese el año en que nacio: '
	Leer nacimiento
	// Cambio de variables en operacion
	ANOS <- actual-nacimiento
	Escribir 'Su Edad es de: ', ANOS, ' años'
FinAlgoritmo
